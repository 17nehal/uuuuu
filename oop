package jotforrm;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class test {
	public static void main(String [] args) throws InterruptedException{
		
		System.setProperty("webdriver.chrome.driver","C:/seleniumtools/chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://form.jotform.com/232900647228052/");
		System.out.println("Selenium Webdriver Script in Chrome browser | SoftwareTesting Material");
		driver.findElement(By.id("first_3")).sendKeys("nehal");
		
}
}



package selenium;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class jotform2 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		System.setProperty("webdriver.chrome.driver","C:/Users/DMC College/Desktop/UPDATED ECLIPSE/chromedriver.exe");
	    WebDriver driver = new ChromeDriver();
	    driver.get("https://form.jotform.com/222571179097059");
	    driver.findElement(By.name("q3_name[first]")).sendKeys("Joism");
	    driver.findElement(By.name("q3_name[last]")).sendKeys("Lobo");
	    driver.findElement(By.name("q4_email")).sendKeys("joismlobo@gmail.com");
	    driver.findElement(By.name("q5_address[addr_line1]")).sendKeys("Soranto Vaddo,Anjuna,Bardez-Goa.");
	    
	    driver.findElement(By.name("q5_address[city]")).sendKeys("Anjuna");
	    driver.findElement(By.name("q5_address[state]")).sendKeys("GOA");
	    driver.findElement(By.name("q5_address[postal]")).sendKeys("403509");
	    driver.findElement(By.name("q6_phoneNumber[full]")).sendKeys("9921449948");
	    driver.findElement(By.id("input_2")).click(); 
	    driver.close();
	}

}

